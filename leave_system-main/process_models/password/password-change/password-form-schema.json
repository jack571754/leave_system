{
  "title": "密码修改申请表",
  "description": "请填写密码修改申请信息",
  "type": "object",
  "required": ["user_email", "user_name", "department", "password_type", "user_level", "reason"],
  "properties": {
    "user_email": {
      "type": "string",
      "title": "申请人邮箱",
      "format": "email"
    },
    "user_name": {
      "type": "string",
      "title": "申请人姓名"
    },
    "department": {
      "type": "string",
      "title": "所属部门"
    },
    "password_type": {
      "type": "string",
      "title": "密码类型",
      "enum": ["normal", "important", "critical"],
      "enumNames": ["普通密码", "重要密码", "关键密码"]
    },
    "user_level": {
      "type": "string",
      "title": "用户级别",
      "enum": ["normal", "admin", "super_admin"],
      "enumNames": ["普通用户", "管理员", "超级管理员"]
    },
    "system_name": {
      "type": "string",
      "title": "系统名称"
    },
    "reason": {
      "type": "string",
      "title": "修改原因"
    }
  }
}
